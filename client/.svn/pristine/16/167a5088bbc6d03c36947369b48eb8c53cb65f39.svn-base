#!/usr/bin/expect

set user [lindex $argv 0]
set ipaddr [lindex $argv 1]
set cmd_str [lindex $argv 2]
set cmd_list [split ${cmd_str} "\|"]
set root_flag 0

set timeout 10

spawn ssh ${user}@${ipaddr}

foreach cmd $cmd_list {
	expect {
		"*$*"	{send "${cmd}\r"}
		"*~>*"	{send "${cmd}\r"}
		"*#*"	{send "${cmd}\r"}
	}
}

expect eof
